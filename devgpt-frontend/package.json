{
  "name": "devgpt-frontend",
  "displayName": "devgpt_frontend",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.gptDebug",
        "title": "DevGPT Debug"
      },
      {
        "command": "extension.gptExplain",
        "title": "DevGPT Explain"
      },
      {
        "command": "extension.gptGoogle",
        "title": "DevGPT Google"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "extension.gptDebug",
          "group": "navigation"
        },
        {
          "command": "extension.gptExplain",
          "group": "navigation"
        },
        {
          "command": "extension.gptGoogle",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.86.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "@vscode/test-cli": "^0.0.6",
    "@vscode/test-electron": "^2.3.9"
  },
  "menus": {
    "editor/context": [
      {
        "when": "editorFocus",
        "command": "extension.sendCodeToBackend",
        "group": "1_modification",
        "context": [
          "editorTextFocus",
          "editorHasSelection"
        ]
      }
    ]
  }
  
}
